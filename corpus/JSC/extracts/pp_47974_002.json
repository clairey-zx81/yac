{
  "$schema": "http://json-schema.org/draft-04/schema",
  "description": "A license for datasets",
  "properties": {
    "created": {
      "format": "date-time",
      "title": "Created",
      "type": "string"
    },
    "identifier": {
      "title": "Identifier",
      "type": "string"
    },
    "link": {
      "description": "A link to the license definition",
      "format": "uri",
      "title": "definition",
      "type": "string"
    },
    "modified": {
      "format": "date-time",
      "title": "Modified",
      "type": "string"
    },
    "title": {
      "title": "Title",
      "type": "string"
    }
  },
  "required": [
    "title",
    "identifier"
  ],
  "title": "License",
  "type": "object"
}
