{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "timeSeries": {
      "description": "Shows the time series in an editable table.",
      "properties": {
        "resource": {
          "axRole": "inlet",
          "description": "Name of the data resource which should refer to a 2-dimensional array.",
          "format": "topic",
          "type": "string"
        }
      },
      "required": [
        "resource"
      ],
      "type": "object"
    },
    "title": {
      "properties": {
        "htmlLabel": {
          "description": "Editable table title",
          "type": "string"
        }
      },
      "type": "object"
    }
  },
  "required": [
    "timeSeries"
  ],
  "type": "object"
}
