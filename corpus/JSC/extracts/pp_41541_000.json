{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "config": {
      "properties": {
        "timezone": {
          "default": "America/Los_Angeles",
          "type": "string"
        }
      },
      "required": [
        "timezone"
      ],
      "type": "object"
    },
    "inputs": {
      "properties": {
        "pfile": {
          "properties": {
            "type": {
              "enum": [
                "pfile"
              ]
            }
          },
          "type": "object"
        }
      },
      "required": [
        "pfile"
      ],
      "type": "object"
    }
  },
  "required": [
    "config",
    "inputs"
  ],
  "title": "Invocation manifest for CNI: GE P-File Metadata Import and Classification",
  "type": "object"
}
