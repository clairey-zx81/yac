{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "additionalProperties": false,
  "allOf": [
    {
      "oneOf": [
        {
          "required": [
            "age_group"
          ]
        },
        {
          "required": [
            "date_of_birth"
          ]
        }
      ]
    }
  ],
  "properties": {
    "age_group": {
      "maxLength": 5,
      "minLength": 5,
      "type": "string"
    },
    "date_of_birth": {
      "maximum": 10,
      "minimum": 10,
      "type": "string"
    }
  },
  "type": "object"
}
