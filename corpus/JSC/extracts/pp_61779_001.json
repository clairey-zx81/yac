{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "arguments_schema": {
      "title": "Arguments schema",
      "type": "string"
    },
    "id": {
      "title": "ID",
      "type": "integer"
    },
    "implementation_url": {
      "format": "uri",
      "title": "Implementation url",
      "type": "string"
    },
    "name": {
      "format": "slug",
      "maxLength": 255,
      "minLength": 1,
      "pattern": "^[-a-zA-Z0-9_]+$",
      "title": "Name",
      "type": "string"
    }
  },
  "required": [
    "arguments_schema",
    "name"
  ],
  "type": "object"
}
