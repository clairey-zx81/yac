{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Person using multi level references",
  "id": "jsdb:person",
  "properties": {
    "address": {
      "$ref": "jsdb:address#"
    },
    "name": {
      "type": "string"
    }
  },
  "required": [
    "name",
    "address"
  ],
  "title": "Person",
  "type": "object"
}
