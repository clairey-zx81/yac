{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "additionalProperties": false,
  "properties": {
    "glob": {
      "default": false,
      "type": "boolean"
    },
    "publish": {
      "type": "object"
    },
    "publisher_type": {
      "enum": [
        "interpolated-pub"
      ],
      "type": "string"
    },
    "relative_paths": {
      "default": false,
      "type": "boolean"
    }
  },
  "required": [
    "publisher_type",
    "publish"
  ],
  "title": "Simple Interpolated Publisher",
  "type": "object"
}
