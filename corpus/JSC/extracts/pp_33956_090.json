{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "definitions": {
    "address_1": {
      "description": "street address line 1",
      "example": "40 Hickory Lane",
      "type": [
        "string"
      ]
    },
    "address_2": {
      "description": "street address line 2",
      "example": "Suite 103",
      "type": [
        "string",
        "null"
      ]
    },
    "card_number": {
      "description": "encrypted card number of payment method",
      "example": "encrypted-card-number",
      "type": [
        "string",
        "null"
      ]
    },
    "city": {
      "description": "city",
      "example": "San Francisco",
      "type": [
        "string"
      ]
    },
    "country": {
      "description": "country",
      "example": "US",
      "type": [
        "string"
      ]
    },
    "created_at": {
      "description": "when the team was created",
      "example": "2012-01-01T12:00:00Z",
      "format": "date-time",
      "readOnly": true,
      "type": [
        "string"
      ]
    },
    "credit_card_collections": {
      "description": "whether charges incurred by the team are paid by credit card.",
      "example": true,
      "readOnly": true,
      "type": [
        "boolean"
      ]
    },
    "cvv": {
      "description": "card verification value",
      "example": "123",
      "type": [
        "string",
        "null"
      ]
    },
    "default": {
      "description": "whether to use this team when none is specified",
      "example": true,
      "readOnly": false,
      "type": [
        "boolean"
      ]
    },
    "device_data": {
      "description": "Device data string generated by the client",
      "example": "VGhpcyBpcyBhIGdvb2QgZGF5IHRvIGRpZQ==",
      "type": [
        "string",
        "null"
      ]
    },
    "enterprise_account": {
      "properties": {
        "id": {
          "$ref": "#/definitions/enterprise-account/definitions/id"
        },
        "name": {
          "$ref": "#/definitions/enterprise-account/definitions/name"
        }
      },
      "type": [
        "null",
        "object"
      ]
    },
    "expiration_month": {
      "description": "expiration month",
      "example": "11",
      "type": [
        "string",
        "null"
      ]
    },
    "expiration_year": {
      "description": "expiration year",
      "example": "2014",
      "type": [
        "string",
        "null"
      ]
    },
    "first_name": {
      "description": "the first name for payment method",
      "example": "Jason",
      "type": [
        "string"
      ]
    },
    "id": {
      "description": "unique identifier of team",
      "example": "01234567-89ab-cdef-0123-456789abcdef",
      "format": "uuid",
      "readOnly": true,
      "type": [
        "string"
      ]
    },
    "identity": {
      "anyOf": [
        {
          "$ref": "#/definitions/team/definitions/name"
        },
        {
          "$ref": "#/definitions/team/definitions/id"
        }
      ]
    },
    "identity_provider": {
      "description": "Identity Provider associated with the Team",
      "properties": {
        "id": {
          "$ref": "#/definitions/identity-provider/definitions/id"
        },
        "slug": {
          "description": "user-friendly unique identifier for this identity provider",
          "example": "acme-sso",
          "type": [
            "string"
          ]
        }
      },
      "strictProperties": true,
      "type": [
        "null",
        "object"
      ]
    },
    "last_name": {
      "description": "the last name for payment method",
      "example": "Walker",
      "type": [
        "string"
      ]
    },
    "membership_limit": {
      "description": "upper limit of members allowed in a team.",
      "example": 25,
      "readOnly": true,
      "type": [
        "number",
        "null"
      ]
    },
    "name": {
      "description": "unique name of team",
      "example": "example",
      "readOnly": true,
      "type": [
        "string"
      ]
    },
    "nonce": {
      "description": "Nonce generated by Braintree hosted fields form",
      "example": "VGhpcyBpcyBhIGdvb2QgZGF5IHRvIGRpZQ==",
      "type": [
        "string",
        "null"
      ]
    },
    "other": {
      "description": "metadata",
      "example": "Additional information for payment method",
      "type": [
        "string",
        "null"
      ]
    },
    "postal_code": {
      "description": "postal code",
      "example": "90210",
      "type": [
        "string"
      ]
    },
    "provisioned_licenses": {
      "description": "whether the team is provisioned licenses by salesforce.",
      "example": true,
      "readOnly": true,
      "type": [
        "boolean"
      ]
    },
    "role": {
      "description": "role in the team",
      "enum": [
        "admin",
        "collaborator",
        "member",
        "owner",
        null
      ],
      "example": "admin",
      "readOnly": true,
      "type": [
        "null",
        "string"
      ]
    },
    "state": {
      "description": "state",
      "example": "CA",
      "type": [
        "string"
      ]
    },
    "type": {
      "description": "type of team.",
      "enum": [
        "enterprise",
        "team"
      ],
      "example": "team",
      "readOnly": true,
      "type": [
        "string"
      ]
    },
    "updated_at": {
      "description": "when the team was updated",
      "example": "2012-01-01T12:00:00Z",
      "format": "date-time",
      "readOnly": true,
      "type": [
        "string"
      ]
    }
  },
