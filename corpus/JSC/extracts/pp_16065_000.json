{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "active": {
      "anyOf": [
        {
          "type": "boolean"
        },
        {
          "type": "null"
        }
      ]
    },
    "dispositionTypeCd": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ]
    },
    "id": {
      "anyOf": [
        {
          "minimum": 0,
          "type": "integer"
        },
        {
          "type": "null"
        }
      ]
    },
    "new": {
      "anyOf": [
        {
          "type": "boolean"
        },
        {
          "type": "null"
        }
      ]
    },
    "personId": {
      "maxLength": 40,
      "type": "string"
    },
    "projectId": {
      "anyOf": [
        {
          "minimum": 0,
          "type": "integer"
        },
        {
          "type": "null"
        }
      ]
    },
    "roleCode": {
      "maxLength": 50,
      "type": "string"
    },
    "sourceIdentifier": {
      "anyOf": [
        {
          "maxLength": 50,
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    },
    "sourcePersonType": {
      "maxLength": 20,
      "type": "string"
    },
    "sourceSystem": {
      "anyOf": [
        {
          "maxLength": 20,
          "type": "string"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "required": [
    "personId",
    "sourcePersonType",
    "roleCode"
  ],
  "type": "object"
}
