{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "A hierarchical collection of datasets who share communications about items",
  "id": "http://www.something.gov/rels/organization",
  "properties": {
    "created": {
      "description": "Timestamp when created",
      "format": "date-time",
      "title": "Created At",
      "type": "string"
    },
    "modified": {
      "description": "Timestamp when last modified",
      "format": "date-type",
      "title": "Modified At",
      "type": "string"
    }
  },
  "title": "Organization",
  "type": "object"
}
