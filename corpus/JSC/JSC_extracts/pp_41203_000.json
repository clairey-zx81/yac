{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "config": {
      "properties": {
        "anat_only": {
          "default": false,
          "type": "boolean"
        },
        "gear-abort-on-bids-error": {
          "default": false,
          "type": "boolean"
        },
        "gear-run-bids-validation": {
          "default": true,
          "type": "boolean"
        },
        "gear-save-output-on-error": {
          "default": false,
          "type": "boolean"
        },
        "mem_gb": {
          "default": 0,
          "type": "number"
        },
        "mem_mb": {
          "default": 0,
          "type": "number"
        },
        "n_cpus": {
          "default": 0,
          "type": "integer"
        },
        "save_working_dir": {
          "default": false,
          "type": "boolean"
        }
      },
      "required": [
        "anat_only",
        "n_cpus",
        "mem_mb",
        "mem_gb",
        "save_working_dir",
        "gear-save-output-on-error",
        "gear-run-bids-validation",
        "gear-abort-on-bids-error"
      ],
      "type": "object"
    },
    "inputs": {
      "properties": {
        "api-key": {
          "type": "object"
        },
        "pipeline_file": {
          "properties": {},
          "type": "object"
        }
