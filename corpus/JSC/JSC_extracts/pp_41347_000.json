{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "properties": {
    "config": {
      "properties": {
        "gear-log-level": {
          "default": "INFO",
          "enum": [
            "ERROR",
            "WARNING",
            "INFO",
            "DEBUG"
          ],
          "type": "string"
        }
      },
      "required": [
        "gear-log-level"
      ],
      "type": "object"
    },
    "inputs": {
      "properties": {
        "image": {
          "properties": {},
          "type": "object"
        }
      },
      "required": [
        "image"
      ],
      "type": "object"
    }
  },
  "required": [
    "config",
    "inputs"
  ],
  "title": "Invocation manifest for OpenSlide to PNG file converter",
  "type": "object"
}
