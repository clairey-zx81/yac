        },
        "geometry": {
            "title": "geometry",
            "description": "One geometry as defined by GeoJSON",
            "type": "object",
            "required": [
                "type",
                "coordinates"
            ],
            "oneOf": [
                {
                    "title": "Point",
                    "properties": {
                        "type": {
                            "enum": [
                                "Point"
                            ]
                        },
                        "coordinates": {
                            "$ref": "#/definitions/position"
                        }
                    }
                },
                {
                    "title": "MultiPoint",
                    "properties": {
                        "type": {
                            "enum": [
                                "MultiPoint"
                            ]
                        },
                        "coordinates": {
                            "$ref": "#/definitions/positionArray"
                        }
                    }
                },
                {
                    "title": "LineString",
                    "properties": {
                        "type": {
                            "enum": [
                                "LineString"
                            ]
                        },
                        "coordinates": {
                            "$ref": "#/definitions/lineString"
                        }
                    }
                },
                {
                    "title": "MultiLineString",
                    "properties": {
                        "type": {
                            "enum": [
                                "MultiLineString"
                            ]
                        },
                        "coordinates": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/lineString"
                            }
                        }
                    }
                },
                {
                    "title": "Polygon",
                    "properties": {
                        "type": {
                            "enum": [
                                "Polygon"
                            ]
                        },
                        "coordinates": {
                            "$ref": "#/definitions/polygon"
                        }
                    }
                },
                {
                    "title": "MultiPolygon",
                    "properties": {
                        "type": {
                            "enum": [
                                "MultiPolygon"
                            ]
                        },
                        "coordinates": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/polygon"
                            }
                        }
                    }
                }
            ],
            "position": {
                "description": "A single position",
                "type": "array",
                "minItems": 2,
                "items": [
                    {
                        "type": "number"
                    },
                    {
                        "type": "number"
                    }
