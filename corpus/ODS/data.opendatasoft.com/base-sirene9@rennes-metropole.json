                    }
                }
            }
        },
        "feature": {
            "title": "Feature",
            "description": "A Geo JSON feature object",
            "required": [
                "geometry",
                "properties"
            ],
            "properties": {
                "type": {
                    "enum": [
                        "Feature"
                    ]
                },
                "geometry": {
