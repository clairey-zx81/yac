                            "description": "Vicinity or development where the streetlight is located"
                        },
                        "address": {
                            "type": "string",
                            "title": "Address",
                            "description": "Address number and street name where streetlight is located"
                        },
                        "dis": {
                            "type": "string",
                            "title": "DIS",
                            "description": "Unique number from Duke Energy Progress for each streetlight"
                        },
                        "source": {
                            "type": "string",
                            "title": "Source",
                            "description": "The entity the pays the bill for the streetlight (DEP = Duke Energy Progress) "
                        },
                        "pole": {
                            "type": "string",
                            "title": "Pole",
                            "description": "Type of pole that supports the streetlight"
                        },
                        "fixture": {
                            "type": "string",
                            "title": "Fixture",
                            "description": "The style or shape of the electrical light fitting device used to hold the bulb in place"
                        },
                        "hid": {
                            "type": "string",
                            "title": "Bulb Type",
                            "description": "Type of bulb"
                        },
                        "cable": {
                            "type": "string",
                            "title": "Cable",
                            "description": "Location of the electrical wiring for the streetlight"
                        },
                        "dateverified": {
                            "type": "string",
                            "format": "date-time",
                            "title": "Date Verified",
                            "description": "Date of streetlight field verification"
                        },
                        "geo_shape": {
                            "type": "object",
                            "oneOf": [
                                {
                                    "$ref": "#/definitions/geometry"
                                }
                            ],
                            "title": "geo_shape",
                            "description": ""
                        },
                        "geo_point_2d": {
                            "type": "array",
                            "minItems": 2,
                            "maxItems": 2,
                            "items": {
                                "type": "number"
                            },
                            "title": "geo_point_2d",
                            "description": ""
                        }
                    }
                }
            }
        }
