{
    "title": "animal-control-inventory-register@vancouver",
    "type": "object",
    "oneOf": [
        {
            "$ref": "#/definitions/animal-control-inventory-register@vancouver"
        }
    ],
    "definitions": {
        "animal-control-inventory-register@vancouver": {
            "properties": {
                "records": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/animal-control-inventory-register@vancouver_records"
                    }
                }
            }
        },
        "animal-control-inventory-register@vancouver_records": {
            "properties": {
                "fields": {
                    "type": "object",
                    "properties": {
                        "animalid": {
                            "type": "string",
                            "title": "AnimalID",
                            "description": "Unique sequential number given to each entry"
                        },
                        "breed": {
                            "type": "string",
                            "title": "Breed",
                            "description": "Type of animal"
                        },
                        "shotsdate": {
                            "type": "string",
                            "format": "date",
                            "title": "ShotsDate",
                            "description": "Date when given vaccination"
                        },
                        "sex": {
                            "type": "string",
                            "title": "Sex",
                            "description": "(M=Male, F=Female, M/N= Male Neutered, F/S= Female Spayed)"
                        },
                        "receiptnumber": {
                            "type": "string",
                            "title": "ReceiptNumber",
                            "description": "Receipt number corresponding to point of sale system of record"
                        },
                        "dateimpounded": {
                            "type": "string",
                            "format": "date",
                            "title": "DateImpounded",
                            "description": "Date when animal was first in the custody of the City of Vancouver"
                        },
                        "pitnumber": {
                            "type": "string",
                            "title": "PitNumber",
                            "description": "Number identifying animal kennel. Never changes while the animal is in the City's custody"
                        },
                        "name": {
                            "type": "string",
                            "title": "Name",
                            "description": "Name of animal if known"
                        },
                        "kennelnumber": {
                            "type": "string",
                            "title": "KennelNumber",
                            "description": "Number of kennel displayed at the top of each kennel"
                        },
                        "dispositiondate": {
                            "type": "string",
                            "format": "date",
                            "title": "DispositionDate",
                            "description": "Date when animal left animal control and no long under the control of the City of Vancouver"
                        },
                        "color": {
                            "type": "string",
                            "title": "Color",
                            "description": "Color of coat"
                        },
                        "code": {
                            "type": "string",
                            "title": "Code",
                            "description": "Walk-ability index for volunteer program (Green=easy, Yellow= moderate, Blue= challenging)"
                        },
                        "approxweight": {
                            "type": "string",
                            "title": "ApproxWeight",
                            "description": "Approximate weight of animal"
                        },
                        "age_category": {
                            "type": "string",
                            "title": "Age category",
                            "description": "Rough estimate of age (puppy, young adult,  adult, senior)"
                        },
                        "source": {
                            "type": "string",
                            "title": "Source",
                            "description": "Where animal came from (Brought-In, Holding Stray, Transferred)"
                        },
                        "status": {
                            "type": "string",
                            "title": "Status",
                            "description": "Current disposition of the animal"
                        },
                        "aco": {
                            "type": "string",
                            "title": "ACO",
                            "description": "Animal control officer number or initial of employee"
                        },
                        "enteredby": {
                            "type": "string",
                            "title": "EnteredBy",
                            "description": "Person who entered data in register"
                        }
                    }
                }
            }
        }
    }
}
