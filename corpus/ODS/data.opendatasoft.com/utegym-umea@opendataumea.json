                        },
                        "id": {
                            "type": "string",
                            "title": "id",
                            "description": "The ID for the outdoor gym"
                        },
                        "namn": {
                            "type": "string",
                            "title": "Name",
                            "description": ""
                        },
                        "lat": {
                            "type": "number",
                            "title": "Latitude",
                            "description": "Latitude in WGS84"
                        },
                        "long": {
                            "type": "number",
                            "title": "Longitude",
                            "description": "Longitud in WGS84"
                        },
                        "epost": {
                            "type": "string",
                            "title": "email",
                            "description": "Email address for further contact"
                        },
                        "webb": {
                            "type": "string",
                            "title": "Visit_url",
                            "description": "Link to the municipality's information page about the outdoor gym(s)"
                        },
                        "wikidata": {
                            "type": "string",
                            "title": "wikidata",
                            "description": "The Q-code of the entity on Wikidata. This field has a value only if users have submitted info on which it should be. You can see how to submit information about this in the Wikidata tab for the dataset. Correctness of submitted value is not checked. The quality of this field is unknown."
                        },
                        "beskrivning": {
                            "type": "string",
                            "title": "Description",
                            "description": "Brief description, in Swedish, of the outdoor gym and its surroundings."
                        },
                        "uppdaterad": {
                            "type": "string",
                            "format": "date-time",
                            "title": "Updated",
                            "description": "Date when the information was last updated in our internal systems"
                        },
                        "geo_point_2d": {
                            "type": "array",
                            "minItems": 2,
                            "maxItems": 2,
                            "items": {
                                "type": "number"
                            },
                            "title": "Geo Point",
                            "description": "Geographical position"
                        }
                    }
                }
            }
        }
