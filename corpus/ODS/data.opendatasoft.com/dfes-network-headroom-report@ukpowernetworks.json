        "dfes-network-headroom-report@ukpowernetworks_records": {
            "properties": {
                "fields": {
                    "type": "object",
                    "properties": {
                        "category": {
                            "type": "string",
                            "title": "Category",
                            "description": "Indicates the type of headroom – unused capacity for demand, for inverter-connected generation or synchronous generation"
                        },
                        "licencearea": {
                            "type": "string",
                            "title": "LicenceArea",
                            "description": "One of the UK Power Networks licence areas - Eastern, London or South Eastern Power Networks "
                        },
                        "sitename": {
                            "type": "string",
                            "title": "SiteName",
                            "description": "Substation name, either a Primary or Bulk Supply Point (Grid) substation owned by UK Power Networks, as listed in the Long Term Development Statement"
                        },
                        "voltage": {
                            "type": "number",
                            "title": "Voltage",
                            "description": "Site Voltage in kV"
                        },
                        "sitefunctionallocation": {
                            "type": "string",
                            "title": "SiteFunctionalLocation",
                            "description": "This is the Site alias in both the Network Control and Asset Management System. It includes the site number"
                        },
                        "bulksupplypoint": {
                            "type": "string",
                            "title": "BulkSupplyPoint",
                            "description": "A substation typically at a voltage level between a Grid Supply Point and a Primary substation "
                        },
                        "gridsupplypoint": {
                            "type": "string",
                            "title": "GridSupplyPoint",
                            "description": "Substation at the interface between the transmission and distribution network"
                        },
                        "committed_capacity": {
                            "type": "number",
                            "title": "BaselineCommittedCapacity(MVA)",
                            "description": "For demand headrooms only, the undiversifed value in MVA of capacity contractually committed to customers which was not being used in the base year."
                        },
                        "scenario": {
                            "type": "string",
                            "title": "Scenario",
                            "description": "Distribution Future Energy Scenario "
                        },
                        "year": {
                            "type": "string",
                            "title": "Year",
                            "description": "Financial year, so 2020 means 2021/22 i.e. 1st April 2020 to 31st March 2021 "
                        },
                        "headroom": {
                            "type": "number",
                            "title": "Headroom",
                            "description": "Unused substation capacity in MW."
                        },
                        "spatial_coordinates": {
                            "type": "array",
                            "minItems": 2,
                            "maxItems": 2,
                            "items": {
                                "type": "number"
                            },
                            "title": "Spatial Coordinates",
                            "description": "Latitude and Longitude, location of the Site."
                        }
                    }
                }
            }
        }
    }
